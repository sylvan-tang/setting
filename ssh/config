#TCPKeepAlive yes
#ServerAliveInterval 30
#StrictHostKeyChecking yes
#UseKeychain yes
#AddKeysToAgent yes
ForwardAgent yes

Host gw1
    HostName bastion001.m6.momo.com
    Port 16020
    User chen.suna
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

Host gw2
    HostName bastion002.m6.momo.com
    Port 16020
    User chen.suna
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

# 该文件用于配置私钥对应的服务器
Host git@bitbucket.org
    HostName https://bitbucket.org
    User SylvanChen
    IdentityFile ~/.ssh/id_rsa_sylvan
    ForwardAgent yes

Host momo
    HostName https://git.wemomo.com
    User ru.sylvan
    IdentityFile ~/.ssh/id_rsa

# 下面的配置用于设置跳板机直接用于登陆
Host lakshmi.gw
    HostName awssgb-platform-lakshmi-web001.stage.awssgb.momo.com
    User chen.suna
    ForwardAgent yes
    ProxyCommand ssh -p 16020 -l chen.suna gw1 -W %h:16020
    ServerAliveInterval 20
    ServerAliveCountMax 30
